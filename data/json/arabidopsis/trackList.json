{
   "tracks" : [
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "chunkSize" : 20000,
         "urlTemplate" : "seq/{refseq_dirpath}/{refseq}-",
         "label" : "TAIR10_genome",
         "type" : "SequenceTrack",
         "key" : "Reference Sequence"
      },
      {
         "style" : {
            "className" : "feature"
         },
         "menuTemplate" : [
            {
               "action" : "iframeDialog",
               "url" : "http://<URI_PLACEHOLDER>/thalemine/keywordSearchResults.do?searchTerm={name}",
               "label" : "Thalemine Gene Report: {name}"
            },
            {
               "action" : "iframeDialog",
               "url" : "http://www.arabidopsis.org/servlets/TairObject?name={name}&type=locus",
               "label" : "TAIR Locus Detail Report: {name}"
            }
         ],
         "key" : "Locus",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : null,
         "urlTemplate" : "tracks/TAIR10_loci/{refseq}/trackData.json",
         "compress" : 0,
         "type" : "FeatureTrack",
         "label" : "TAIR10_loci"
      },
      {
         "style" : {
            "className" : "transcript",
            "subfeatureClasses" : {
               "three_prime_UTR" : "transcript-UTR",
               "exon" : "transcript-exon",
               "five_prime_UTR" : "transcript-UTR",
               "CDS" : "transcript-CDS"
            }
         },
         "menuTemplate" : [
            {
               "action" : "contentDialog",
               "content" : "<table border=1><tr><td>Identifier</td><td>{name}</td></tr><tr><td>Name</td><td>{note}</td></tr><tr><td><strong>Confidence Class (1-10)</strong></td><td>{conf_class}</td></tr><tr><td><strong>Confidence Rating (star)</strong></td><td>{conf_rating}</td></tr><tr><td><strong>Computational description</strong></td><td>{computational_description}</td></tr><tr><td><strong>Curatory summary</strong></td><td>{curator_summary}</td></tr></table>",
               "title" : "Gene Info: {name}",
               "label" : "Gene Info: {name}"
            },
            {
               "iconClass" : "dijitIconDatabase",
               "action" : "iframeDialog",
               "url" : "http://<URI_PLACEHOLDER>/thalemine/keywordSearchResults.do?searchTerm={name}",
               "title" : "Thalemine Gene Report: {name}",
               "label" : "Thalemine Gene Report: {name}"
            },
            {
               "iconClass" : "dijitIconDatabase",
               "action" : "iframeDialog",
               "url" : "http://www.arabidopsis.org/servlets/TairObject?name={name}&type=gene",
               "title" : "TAIR Gene Model Detail Report: {name}",
               "label" : "TAIR Gene Model Detail Report: {name}"
            }
         ],
         "hooks" : {
            "modify" : "function( track, feature, div ) { var Note = feature.get('Note'); if(Note === 'transposable element gene') { dojo.map(div.children, function(child) { if (child.className.match(/subfeature/)) child.className = child.className + ' te-exon'; }); } }"
         },
         "key" : "Protein Coding Gene Models",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : null,
         "urlTemplate" : "tracks/TAIR10_genes/{refseq}/trackData.json",
         "compress" : 0,
         "label" : "TAIR10_genes",
         "type" : "FeatureTrack"
      },
      {
         "style" : {
            "className" : "generic_parent",
            "subfeatureClasses" : {
               "transposon_fragment" : "feature3"
            }
         },
         "menuTemplate" : [
            {
               "action" : "iframeDialog",
               "url" : "http://www.arabidopsis.org/servlets/TairObject?name={name}&type=transposon",
               "label" : "TAIR Transposon Detail Report"
            }
         ],
         "key" : "Natural Transposons",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : null,
         "urlTemplate" : "tracks/TAIR10_transposons/{refseq}/trackData.json",
         "compress" : 0,
         "type" : "FeatureTrack",
         "label" : "TAIR10_transposons"
      },
      {
         "style" : {
            "className" : "exon"
         },
         "hooks" : {
            "modify" : "function( track, feature, div ) { var Note = feature.get('Note'); if(Note.match(/E3|X2|X3|S2|S3/)) { bgcolor = 'pink'; } else if (Note.match(/E4|X4/)) { bgcolor = 'hotpink'; } else if(Note.match(/E5/)) { bgcolor = 'red'; } else if(Note.match(/E7|X5|S4/)) { bgcolor = 'darkred'; } div.style.backgroundColor = bgcolor; }"
         },
         "key" : "Unconfirmed Exons",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : null,
         "urlTemplate" : "tracks/TAIR10_unconfirmed_exons/{refseq}/trackData.json",
         "compress" : 0,
         "type" : "FeatureTrack",
         "label" : "TAIR10_unconfirmed_exons"
      },
      {
         "style" : {
            "className" : "generic_parent",
